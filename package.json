{
  "name": "express-api-es6-starter",
  "version": "1.0.0",
  "description": "Express API ES6 Starter",
  "scripts": {
    "start": "node dist",
    "prestart": "npm run build",
    "lint": "eslint src; exit 0",
    "build": "rimraf dist && babel src -D --out-dir dist",
    "start:dev": "nodemon --exec babel-node src --source-maps"
  },
  "keywords": [
    "javascript",
    "node",
    "express",
    "es6",
    "api"
  ],
  "private": true,
  "author": "Saugat Acharya <mesaugat@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "helmet": "^3.2.0",
    "knex": "^0.12.6",
    "morgan": "^1.7.0",
    "pg": "^6.1.2",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "eslint": "^3.12.2",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4"
  },
  "engines": {
    "node": ">= 6.5.0",
    "npm": ">= 3.10.0"
  }
}
